type AutoGenerated []struct {
	Name    string `json:"name"`
	Type    string `json:"type"`
	Options struct {
		Validator struct {
			JSONSchema struct {
				BsonType   string   `json:"bsonType"`
				Required   []string `json:"required"`
				Properties struct {
					Domain struct {
						BsonType    string `json:"bsonType"`
						Description string `json:"description"`
					} `json:"domain"`
					Label struct {
						BsonType    string `json:"bsonType"`
						Description string `json:"description"`
					} `json:"label"`
					Model struct {
						BsonType    string `json:"bsonType"`
						Description string `json:"description"`
					} `json:"model"`
					Pins struct {
						Items struct {
							Title       string   `json:"title"`
							BsonType    string   `json:"bsonType"`
							Description string   `json:"description"`
							Required    []string `json:"required"`
							UniqueItems bool     `json:"uniqueItems"`
							Properties  struct {
								ID struct {
									BsonType    string `json:"bsonType"`
									Description string `json:"description"`
								} `json:"id"`
								Label struct {
									BsonType    string `json:"bsonType"`
									Description string `json:"description"`
								} `json:"label"`
								Purpose struct {
									Description string `json:"description"`
									BsonType    string `json:"bsonType"`
								} `json:"purpose"`
							} `json:"properties"`
						} `json:"items"`
						BsonType    string `json:"bsonType"`
						Description string `json:"description"`
					} `json:"pins"`
				} `json:"properties"`
			} `json:"$jsonSchema"`
		} `json:"validator"`
	} `json:"options"`
	Info struct {
		ReadOnly bool   `json:"readOnly"`
		UUID     string `json:"uuid"`
	} `json:"info"`
	IDIndex struct {
		V   int `json:"v"`
		Key struct {
			ID int `json:"_id"`
		} `json:"key"`
		Name string `json:"name"`
	} `json:"idIndex"`
}

type Schema struct {
	Title       string   `json:"title"`
	BsonType    string   `json:"bsonType"`
	Description string   `json:"description"`
	Required    []string `json:"required"`
	UniqueItems bool     `json:"uniqueItems"`
	Properties  []Schema   `json:"properties"`
}

