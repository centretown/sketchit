        },
        "label": {
          "type": "string",
          "title": "The label assigned to this pin, eg: \"Internal-LED\""
        },
        "purpose": {
          "type": "string",
          "title": "The pins overall purpose in the sketch,  eg: \"blink on to indicate activity\""
        }
      },
      "title": "Identifies a pin on a micro controller device"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "runtimeError": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        },
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1Device": {
      "type": "object",
      "properties": {
        "domain": {
